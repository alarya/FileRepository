<Window x:Class="WPFClient.Login"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Login" Height="300" Width="300">
    <Grid>
        <Label x:Name="UserName" Content="UserName" HorizontalAlignment="Left" Margin="30,83,0,0" VerticalAlignment="Top" Width="69"/>
        <Label x:Name="PasswordLabel" Content="Password" HorizontalAlignment="Left" Margin="30,148,0,0" VerticalAlignment="Top" RenderTransformOrigin="-0.161,-0.052" Height="25" Width="82">
            <Label.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="0.202"/>
                    <TranslateTransform/>
                </TransformGroup>
            </Label.RenderTransform>
        </Label>
        <TextBox x:Name="userId" HorizontalAlignment="Left" Height="34" Margin="138,83,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="122"/>
        <Button Content="Login" HorizontalAlignment="Left" Margin="102,206,0,0" VerticalAlignment="Top" Width="90" Height="37" Click="LoginButton"/>
        <PasswordBox x:Name="password" HorizontalAlignment="Left" Margin="138,147,0,0" VerticalAlignment="Top" Width="122" Height="33"/>

    </Grid>
</Window>
